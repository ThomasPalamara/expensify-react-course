/*{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0,
        "linebreak-style": 0,
        "comma-dangle":"warn",
        "indent": [2, "tab"],
        "react/jsx-indent": [2, "tab"],
        "react/jsx-indent-props": [2, "tab"],
        "no-tabs": 0,
        "no-unused-vars": [
            "error",
            {
                "vars": "local",
                "args": "none"
            }
        ],
        "import/extensions": "off",
        "import/no-unresolved": "off"
    },
    "settings": {
        "import/resolver": {
            "node": {
              "extensions": [".js", ".json"]
            }
          },
      }
}